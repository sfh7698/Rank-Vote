{
    "name": "rank-vote",
    "version": "1.0.0",
    "description": "Real time voting app",
    "main": "index.js",
    "workspaces": [
        "server",
        "shared",
        "client"
    ],
    "scripts": {
        "server": "npm run dev --workspace=server",
